        <table class="main-table table table-bordered">
                <thead>
                  <tr align="center" bgcolor="#add8e6">
                    <th>Test Name</th>
                    <th>Test Date</th>
                    <th>Result</th>     
                    <th>Abnormal (Y/N)</th>     
                    <th></th>
                  </tr>
                </thead>
      
                <tbody>
                        <% result.forEach((patient, index) => { %>
                            <tr>
                               <td><%=patient.test_name%></td>
                               <td><%=patient.date_completed%></td>
                               <td><%=patient.result%></td>
                               <td><%=patient.abnormal%></td>
                               <td>
                                    <a href="/patient/<%= getPatient_hcard%>/<%=patient.test_name%>/LabUpdate" class="btn btn-sm btn-success">Edit</a>
                                <a href="" class="btn btn-sm btn-danger">Delete</a>
                               </td>
                             </tr>
                             <% }) %>
                </tbody>
              </table>
</div>

